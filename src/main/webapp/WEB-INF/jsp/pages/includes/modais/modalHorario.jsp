<script>
function modalHorario(horario){
	var valor = horario;
	valor = 'Data: '+valor.replace('T','<br><br>Horário: ');
	document.getElementById("horario").innerHTML = valor;
	$("#modalHorario").modal().show();
}
</script>

<div class="modal fade" id="modalHorario" tabindex="-1" role="dialog" aria-labelledby="TituloModalCentralizado" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="TituloModalCentralizado">Horário</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Fechar">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
       <h5>Horário de entrada do usuário: <br><br> <a id="horario"></a></h5>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
      </div>
    </div>
  </div>
</div>
<!--  FINAL CONTATO -->

